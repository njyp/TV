server=="cloudflare" && port="443" && is_domain=false && banner="CF-RAY: -" && banner="Content-Length: 155" && banner="HTTP/1.1 400 Bad Request" && protocol="http" && country=="HK"

server=="cloudflare" && port="443" && is_domain=false && banner="CF-RAY: -" && banner="Content-Length: 155" && banner="HTTP/1.1 400 Bad Request" && protocol="http" && country=="JP"

server=="cloudflare" && port="443" && is_domain=false && banner="CF-RAY: -" && banner="Content-Length: 155" && banner="HTTP/1.1 400 Bad Request" && protocol="http" && country=="KR"

阿里云： server=="cloudflare"&&asn=="45102"

甲骨文韩国：server=="cloudflare"&&asn=="31898"&&country=="KR"  

反代ip：
server!="cloudflare" && port=="443" && country=="US"
server!="cloudflare" && port=="443" && region=="HK"

./CloudflareST -f US-443.txt -tp 443
./CloudflareST -f HK-443.txt -tp 443
